<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tashinfx.HomePageController">
   <children>
      <TextField fx:id="nameField" alignment="CENTER" layoutX="32.0" layoutY="36.0" opacity="0.92" text="Enter Your Name" />
      <Button layoutX="192.0" layoutY="36.0" mnemonicParsing="false" onMouseClicked="#nameSubmit" text="Submit" textAlignment="CENTER" />
      <Label layoutX="404.0" layoutY="31.0" prefHeight="34.0" prefWidth="96.0" text="Attempts left">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Label fx:id="attampLable" layoutX="509.0" layoutY="38.0" text="2">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="166.0" layoutY="88.0" prefHeight="54.0" prefWidth="267.0" text="Press the button to Generate the Number!">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="generateButton" layoutX="270.0" layoutY="142.0" mnemonicParsing="false" onMouseClicked="#generateNumber" text="Number" textFill="RED" />
      <Label fx:id="messageField" layoutX="86.0" layoutY="175.0" prefHeight="18.0" prefWidth="427.0" textFill="RED" />
      <Circle fx:id="numberOne" fill="DODGERBLUE" layoutX="107.0" layoutY="297.0" onMouseClicked="#clickedInOne" radius="60.0" stroke="BLACK" strokeType="INSIDE" />
      <Circle fx:id="numberTwo" fill="DODGERBLUE" layoutX="300.0" layoutY="297.0" onMouseClicked="#clickedInTwo" radius="60.0" stroke="BLACK" strokeType="INSIDE" />
      <Circle fx:id="numberThree" fill="DODGERBLUE" layoutX="483.0" layoutY="297.0" onMouseClicked="#clickedInThree" radius="60.0" stroke="BLACK" strokeType="INSIDE" />
   </children>
</AnchorPane>
